{
  "name": "admag",
  "description": "ADMAG - Electron Application",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Muzammil Shakir",
    "email": "muzammil.shakir786@gmail.com",
    "url": "https://github.com/muzammilshakir"
  },
  "build": {
    "appId": "com.my-website.admag",
    "productName": "ADMAG - Billboard",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-do-expressions": "^7.16.5",
    "@brettcate/s3-react-player": "^2.6.2",
    "@date-io/date-fns": "^2.11.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.2.4",
    "aws-sdk": "^2.1118.0",
    "axios": "^0.24.0",
    "bootstrap": "5.1.3",
    "cron": "^1.8.2",
    "electron-is-dev": "^1.0.1",
    "moment": "^2.29.1",
    "node-cron": "^3.0.0",
    "notistack": "^2.0.3",
    "react": "latest",
    "react-bootstrap": "^2.0.3",
    "react-dom": "latest",
    "react-player": "^2.9.0",
    "react-query": "^3.34.5",
    "react-scripts": "2.1.5",
    "ytdl-core": "^4.9.1"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "start-electron": "wait-on http://localhost:3000 && electron .",
    "electron-dev": "concurrently \"BROWSER=none&&npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev-win": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -l"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@rescripts/cli": "^0.0.10",
    "@rescripts/rescript-env": "^0.0.5",
    "concurrently": "^6.4.0",
    "electron": "^4.0.6",
    "electron-builder": "^20.38.5",
    "typescript": "^3.3.3333",
    "wait-on": "^3.2.0"
  }
}
